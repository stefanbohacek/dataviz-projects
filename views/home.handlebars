<div class="container pb-5">
  <h1 class="mt-5">
    Dataviz projects
  </h1>
  <div class="row list mt-5">
    {{#each projects}}
      <div
        class="col-sm-12 col-md-6 col-lg-4 list-item"
        id="project- {{@index}}"
      >
        <div class="card pb-2 mb-4">
          <a href="{{this.path}}">
            <img
              width="1200"
              src="{{this.path}}/images/preview.png"
              class="card-img-top"
              alt=""
            />
          </a>
          <div class="card-body">
            <a class="text-decoration-none" href="{{this.path}}">
              <h5 class="card-title text-dark">{{this.title}}</h5>
            </a>
            <p class="card-text">{{this.description}}</p>
            {{!-- <p class="card-text text-muted">{{this.publish_date}}</p> --}}
          </div>
        </div>
      </div>
    {{/each}}
  </div>
  <p class="mt-3">
    <a class="btn btn-dark" href="https://stefanbohacek.com/dataviz/">Browse
      more</a>
  </p>
</div>