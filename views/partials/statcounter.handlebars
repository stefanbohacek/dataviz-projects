{{#if sc_project}}
<script type="text/javascript">
var sc_project="{{sc_project}}"; 
var sc_invisible=1; 
var sc_security="{{sc_security}}"; 
</script>
<script type="text/javascript"
src="https://www.statcounter.com/counter/counter.js"
async></script>
